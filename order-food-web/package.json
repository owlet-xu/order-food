{
  "name": "order-food-web",
  "version": "6.0.0",
  "scripts": {
    "start": "ng serve --open --port 8020",
    "build": "ng build --output-path build --stats-json",
    "dist": "ng build --prod",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "analyzer": "webpack-bundle-analyzer build/stats.json",
    "precommit": "npm run lint",
    "prepush": "npm run build",
    "commitmsg": "validate-commit-msg"
  },
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "@gsafety/owlet-config": "0.0.1",
    "@gsafety/owlet-odata": "0.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/swiper": "^4.2.0",
    "angular2-useful-swiper": "^5.0.1",
    "core-js": "^2.5.4",
    "echarts": "^4.1.0",
    "jsnlog": "^2.26.2",
    "less": "^2.7.3",
    "ng-zorro-antd": "^1.0.0",
    "ngx-echarts": "^3.1.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular2-swiper": "^0.8.1",
    "codelyzer": "~4.2.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "swiper": "^4.3.3",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "themes",
        "i18n",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "helpMessage": "does not match '<type>: <subject>' !"
    }
  }
}
